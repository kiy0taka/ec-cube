{
    "name": "ec-cube/ec-cube",
    "description": "EC-CUBE EC open platform.",
    "type": "project",
    "homepage": "http://www.ec-cube.net/",
    "license": [
       "GPL-2.0",
       "Commercial"
    ],
    "support": {
        "issues": "https://github.com/EC-CUBE/ec-cube/issues"
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/EC-CUBE/sensio-framework-extra-service-provider"
        }
    ],
    "require": {
        "php": "^7.0.8",
        "ext-intl": "^1.1",
        "ext-mbstring": "*",
        "composer/ca-bundle": "^1.0",
        "composer/composer": "^1.5",
        "dflydev/doctrine-orm-service-provider": "^2.0",
        "doctrine/data-fixtures": "^1.2",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "doctrine/migrations": "^1.5",
        "doctrine/orm": "^2.5",
        "egulias/email-validator": "^2.1",
        "guzzlehttp/guzzle": "^6.2",
        "hirak/prestissimo": "^0.3.7",
        "knplabs/console-service-provider": "^2.1",
        "knplabs/knp-components": "^1.3",
        "mobiledetect/mobiledetectlib": "^2.8",
        "nesbot/carbon": "^1.22",
        "psr/log": "^1.0",
        "saxulum/saxulum-doctrine-orm-manager-registry-provider": "^3.1",
        "saxulum/saxulum-validator-provider": "^2.1",
        "saxulum/saxulum-webprofiler-provider": "^2.0",
        "sensio/framework-extra-bundle": "^5.1",
        "sensio/generator-bundle": "^3.1",
        "silex/silex": "^2.2",
        "silex/web-profiler": "^2.0",
        "swiftmailer/swiftmailer": "^6.0",
        "symfony/asset": "^3.4",
        "symfony/class-loader": "^3.4",
        "symfony/config": "^3.4",
        "symfony/console": "^3.4",
        "symfony/css-selector": "^3.4",
        "symfony/debug-pack": "^1.0",
        "symfony/doctrine-bridge": "^3.4",
        "symfony/dom-crawler": "^3.4",
        "symfony/dotenv": "^3.4",
        "symfony/event-dispatcher": "^3.4",
        "symfony/filesystem": "^3.4",
        "symfony/finder": "^3.4",
        "symfony/flex": "^1.0",
        "symfony/form": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/http-foundation": "^3.4",
        "symfony/http-kernel": "^3.4",
        "symfony/inflector": "^3.4",
        "symfony/intl": "^3.4",
        "symfony/lts": "^3.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/monolog-bridge": "^3.4",
        "symfony/monolog-bundle": "^3.1",
        "symfony/options-resolver": "^3.4",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "^3.4",
        "symfony/property-access": "^3.4",
        "symfony/routing": "^3.4",
        "symfony/security": "^3.4",
        "symfony/security-bundle": "^3.4",
        "symfony/serializer": "^3.4",
        "symfony/stopwatch": "^3.4",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/templating": "^3.4",
        "symfony/translation": "^3.4",
        "symfony/twig-bundle": "^3.4",
        "symfony/validator": "^3.4",
        "symfony/var-dumper": "^3.4",
        "symfony/web-profiler-bundle": "^3.4",
        "symfony/yaml": "^3.4",
        "white-october/pagerfanta-bundle": "^1.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.2",
        "fzaninotto/faker": "^1.7",
        "mikey179/vfsStream": "^1.6",
        "satooshi/php-coveralls": "2.0.x-dev",
        "symfony/browser-kit": "^3.4",
        "symfony/web-server-bundle": "^3.4",
        "dama/doctrine-test-bundle": "^4.0"
    },
    "autoload": {
        "files": [
            "src/Eccube/Resource/functions/log.php",
            "src/Eccube/Resource/functions/env.php"
        ],
        "psr-4": {
            "Eccube\\Entity\\": "app/proxy/entity",
            "Acme\\": "app/Acme",
            "Eccube\\": "src/Eccube",
            "Plugin\\": "app/Plugin",
            "Dbtlr\\MigrationProvider\\Provider\\": "src/silex-doctrine-migrations"
        },
        "exclude-from-classmap": ["**/Test/**", "**/Tests/**", "**/test/**", "**/tests/**"]
    },
    "autoload-dev" : {
        "psr-4": {
            "Eccube\\Tests\\" : "tests/Eccube/Tests"
        }
    },
    "scripts": {
        "compile": [
            "php eccube_install.php pgsql none --skip-createdb -V"
        ],
        "auto-scripts": {
            "cache:clear --no-warmup": "symfony-cmd",
            "cache:warmup": "symfony-cmd",
            "assets:install --symlink --relative html": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C0Q71D54BCVSB8ZWR3VECDRD",
            "allow-contrib": false
        },
        "symfony-web-dir": "."
    },
    "config": {
        "platform": {
            "php": "7.0.8"
        },
        "preferred-install": {
            "*": "dist"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
